  "source": "Logo Objects REST Service Documentation",
  "docsUrl": "https://docs.logo.com.tr/public/wua/logo-objects-rest-servis",
  "baseUrl": "/api/v1",
  "authentication": {
    "type": "OAuth2",
    "tokenEndpoint": "/token",
    "grantType": "password",
    "bodyFormat": "grant_type=password&username={{username}}&firmno={{firmNo}}&password={{password}}"
  },
  "methods": [
    {
      "name": "GET",
      "description": "Verileri okumak icin kullanilir",
      "color": "#61affe",
      "usage": [
        "Tekil kayit: /api/v1/{resource}/{code}",
        "Alt sorgular: /api/v1/{resource}/{code}/{subResource}",
        "Tam veri: /api/v1/{resource}/{code}?ExpandLevel=full"
      ]
    },
    {
      "name": "POST",
      "description": "Yeni veri olusturmak icin kullanilir",
      "color": "#49cc90",
      "usage": [
        "Yeni kayit: /api/v1/{resource} + JSON body"
      ]
    },
    {
      "name": "PUT",
      "description": "Mevcut kaydi tamamen guncellemek icin kullanilir",
      "color": "#fca130",
      "usage": [
        "GET ile veri alinir, degistirilir ve PUT ile geri gonderilir",
        "Guncelleme: /api/v1/{resource}/{code} + JSON body"
      ]
    },
    {
      "name": "PATCH",
      "description": "INTERNAL_REFERENCE ile belirli alanlari guncellemek icin kullanilir",
      "color": "#50e3c2",
      "usage": [
        "Kismi guncelleme: /api/v1/{resource} + {\"INTERNAL_REFERENCE\":\"xxx\", \"FIELD\":\"value\"}"
      ]
    },
    {
      "name": "DELETE",
      "description": "Kayit silmek icin kullanilir",
      "color": "#f93e3e",
      "usage": [
        "Silme: /api/v1/{resource}/{code}"
      ]
    }
  ],
