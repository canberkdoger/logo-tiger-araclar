  "commonQueries": [
    {
      "name": "Tum Aktif Malzemeler",
      "description": "Kullanimda olan tum malzemeleri listeler",
      "sql": "SELECT CODE, NAME, CARDTYPE, VAT FROM LG_{firmNo}_ITEMS WHERE ACTIVE = 0 ORDER BY CODE"
    },
    {
      "name": "Ticari Mallar",
      "description": "Sadece ticari mal tipindeki kartlari listeler",
      "sql": "SELECT CODE, NAME, VAT, STGRPCODE FROM LG_{firmNo}_ITEMS WHERE ACTIVE = 0 AND CARDTYPE = 1 ORDER BY CODE"
    },
    {
      "name": "Hammaddeler",
      "description": "Sadece hammadde tipindeki kartlari listeler",
      "sql": "SELECT CODE, NAME, UNITSETREF FROM LG_{firmNo}_ITEMS WHERE ACTIVE = 0 AND CARDTYPE = 10 ORDER BY CODE"
    },
    {
      "name": "Malzeme Detayi",
      "description": "Belirli bir malzemenin tum bilgilerini getirir",
      "sql": "SELECT * FROM LG_{firmNo}_ITEMS WHERE CODE = '{malzemeKod}'"
    }
